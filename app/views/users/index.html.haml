- provide(:title, "Users")
%h1 Users
.users
  %ol
    - @users.each do |user|
      %li
        Name:
        = user.name
      %li
        Email:
        = user.email

        = link_to 'Edit |', edit_user_path(user)
        = link_to 'View |', user_path(user)
        = link_to 'Delete', user, method: :delete
      %br/
